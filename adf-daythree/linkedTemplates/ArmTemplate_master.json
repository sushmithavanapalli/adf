{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-daythree"
		},
		"ls_blobinput_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blobinput'"
		},
		"ls_sqldestination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqldestination'"
		},
		"ls_privateendpoint_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_privateendpoint'"
		},
		"AzureSqlDatabasePrivateEndpoint_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/95cd2b52-5b17-466a-8e14-e34779562112/resourceGroups/daythreeadf/providers/Microsoft.Sql/servers/daythreeadfserver"
		},
		"AzureSqlDatabasePrivateEndpoint_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"AzureSqlDatabasePrivateEndpoint_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"daythreeadfserver.database.windows.net"
			]
		},
		"AzureBlobStoragepep_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/95cd2b52-5b17-466a-8e14-e34779562112/resourceGroups/daythreeadf/providers/Microsoft.Storage/storageAccounts/daythreeadfsc"
		},
		"AzureBlobStoragepep_properties_groupId": {
			"type": "string",
			"defaultValue": "blob"
		},
		"AzureBlobStoragepep_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"daythreeadfsc.blob.core.windows.net"
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-daythree_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blobinput_connectionString": {
						"value": "[parameters('ls_blobinput_connectionString')]"
					},
					"ls_sqldestination_connectionString": {
						"value": "[parameters('ls_sqldestination_connectionString')]"
					},
					"ls_privateendpoint_connectionString": {
						"value": "[parameters('ls_privateendpoint_connectionString')]"
					},
					"AzureSqlDatabasePrivateEndpoint_properties_privateLinkResourceId": {
						"value": "[parameters('AzureSqlDatabasePrivateEndpoint_properties_privateLinkResourceId')]"
					},
					"AzureSqlDatabasePrivateEndpoint_properties_groupId": {
						"value": "[parameters('AzureSqlDatabasePrivateEndpoint_properties_groupId')]"
					},
					"AzureSqlDatabasePrivateEndpoint_properties_fqdns": {
						"value": "[parameters('AzureSqlDatabasePrivateEndpoint_properties_fqdns')]"
					},
					"AzureBlobStoragepep_properties_privateLinkResourceId": {
						"value": "[parameters('AzureBlobStoragepep_properties_privateLinkResourceId')]"
					},
					"AzureBlobStoragepep_properties_groupId": {
						"value": "[parameters('AzureBlobStoragepep_properties_groupId')]"
					},
					"AzureBlobStoragepep_properties_fqdns": {
						"value": "[parameters('AzureBlobStoragepep_properties_fqdns')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}